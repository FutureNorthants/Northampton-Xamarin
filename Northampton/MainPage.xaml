<?xml version="1.0" encoding="utf-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Northampton.MainPage" xmlns:local="clr-namespace:Northampton;assembly=Northampton" Title="TabbedPage">
    <NavigationPage Title="Report" Icon="reportit.png">
        <x:Arguments>
            <local:ReportMenu />
        </x:Arguments>
    </NavigationPage>
    <ContentPage Title="Find" Icon="find.png">
        <StackLayout>
            <Label Text="Green" HorizontalTextAlignment="Center" HorizontalOptions="FillAndExpand" Margin="5" />
            <BoxView Color="Green" VerticalOptions="FillAndExpand" />
        </StackLayout>
    </ContentPage>
    <NavigationPage Title="Settings" Icon="settings.png">
        <x:Arguments>
            <local:SettingsMenu />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>